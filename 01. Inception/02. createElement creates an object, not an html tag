
const heading=React.createElement("h1",{id:"head"},"Hello React..!");         // creates an object, not an html tag with content
console.log(heading);                                                         // prints an object
const root= ReactDOM.createRoot(document.getElementById("root"));
root.render(heading);                                                         // purpose of render method is to convert this object sent to it, into an html tag :) and put it into the root html element.






